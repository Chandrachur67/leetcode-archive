generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model contest {
  title_slug String    @id
  title      String
  start_time Int
  done       Boolean   @default(false)
  problems   problem[]
}

model problem {
  title_slug             String  @id
  title                  String
  credit                 Int
  contest_title_slug     String
  contest                contest @relation(fields: [contest_title_slug], references: [title_slug])
}
