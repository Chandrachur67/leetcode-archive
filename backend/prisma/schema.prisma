generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Contest {
  titleSlug String    @id
  title     String
  startTime Int
  done      Boolean   @default(false)
  problems  Problem[]
}

model Problem {
  titleSlug        String  @id
  title            String
  credit           Int
  contestTitleSlug String
  contest          Contest @relation(fields: [contestTitleSlug], references: [titleSlug])
}
